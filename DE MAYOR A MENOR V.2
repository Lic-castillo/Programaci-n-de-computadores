Option Explicit

Sub main3()
    Dim n() As Double
    Dim fil As Long
    
    Call lee("hoja1", n, fil)
    Call escr("hoja2", n, fil)
End Sub

Sub lee(archivo, n, fil)
    Dim i As Long
    Dim k As Long
    Dim pos As Long
    Dim may As Double
    Dim aux As Double
    
    Worksheets(archivo).Select
    i = 1
    While (Cells(i, 1) <> "")
        i = i + 1
    Wend
    fil = i - 1
    
    ReDim n(fil)
    
    For i = 1 To fil Step 1
        n(i) = Cells(i, 1)
    Next i
    
    For k = 1 To fil Step 1
        may = n(k)
        pos = k
        
        For i = k To fil Step 1
            If n(i) > may Then
                may = n(i)
                pos = i
            End If
        Next i
        
        aux = n(k)
        n(k) = n(pos)
        n(pos) = aux
    Next k
End Sub

Sub escr(archivo, n, fil)
    Dim i As Long
    
    Worksheets(archivo).Select
    For i = 1 To fil Step 1
        Cells(i, 1) = n(i)
    Next i
End Sub
