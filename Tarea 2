Option Explicit

Sub principal()
    Dim suma As Long
    Dim TermSig As Long
    Dim n As Double
    
    suma = 0
    For n = 7 To 1000 Step 1
        TermSig = (n ^ 2.1) * 3 / n
        suma = suma + TermSig
    Next n
    
    MsgBox "El total es: " & suma
End Sub

Sub secundario()
    Dim suma As Long
    Dim TermSig As Long
    Dim n As Double
    
    suma = 0
    For n = 0 To 100 Step 1
        TermSig = (n ^ 2) * 3 / n
        suma = suma + TermSig
    Next n
    
    MsgBox "El total de la suma es: " & suma
End Sub
