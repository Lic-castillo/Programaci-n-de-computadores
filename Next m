Private Sub Calculo_Click()
    Dim n As Long
    Dim m As Long
    Dim Primo As Boolean
    
    n = InputBox("n")
    
    Primo = True
    For m = 2 To n - 1
        If (Int(n / m) * m = n) Then
            Primo = False
        End If
    Next m
        
    If (Primo = True) Then
        cout1.AddItem n & " es primo"
        End If
End Sub
