Option Explicit
Sub main2()
          Dim n1 As Double
          Dim n2 As Double
          Dim n3 As Double
          Dim n4 As Double
          Dim n5 As Double
          Dim n6 As Double
          Dim n7 As Double
          Dim n8 As Double
          Dim n9 As Double
          Dim n10 As Double
          Dim n11 As Double
          Dim n12 As Double
          Dim n13 As Double
          Dim n14 As Double
          Dim n15 As Double
            
          Worksheets("hoja1").Select
          n1 = Cells(1, 1)
          n2 = Cells(2, 1)
          n3 = Cells(3, 1)
          n4 = Cells(4, 1)
          n5 = Cells(5, 1)
          
          If (n1 > n2) And (n1 > n3) And (n1 > n4) And (n1 > n5) And (n1 > n6) And (n1 > n7) And (n1 > n8) And (n1 > n9) And (n1 > n10) And (n1 > n11) And (n1 > n12) And (n1 > n13) And (n1 > n14) And (n1 > n15) Then
                    MsgBox (n1 & ", el primer número,  es el mayor de todos")
          End If
          
