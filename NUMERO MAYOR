                    MsgBox (n5 & ", el quinto número, es el mayor de todos")
          End If
          
          If (n6 > n2) And (n6 > n3) And (n6 > n4) And (n6 > n5) And (n6 > n1) And (n6 > n7) And (n6 > n8) And (n6 > n9) And (n6 > n10) And (n6 > n11) And (n6 > n12) And (n6 > n13) And (n6 > n14) And (n6 > n15) Then
                    MsgBox (n6 & ", el sexto número,  es el mayor de todos")
          End If
          
          If (n7 > n1) And (n7 > n3) And (n7 > n4) And (n7 > n5) And (n7 > n6) And (n7 > n2) And (n7 > n8) And (n7 > n9) And (n7 > n10) And (n7 > n11) And (n7 > n12) And (n7 > n13) And (n7 > n14) And (n7 > n15) Then
                    MsgBox (n7 & ", el séptimo número, es el mayor de todos")
          End If
          
