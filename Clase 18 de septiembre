Option Explicit
Sub main()
    Dim a  As Double
    Dim b As Double
    Dim c As Double
    Dim d As Double
    Dim e As Double
    Dim suma As Double
    Dim promedio As Double
    Dim cuadradoSuma As Double
    
    a = InputBox("a")
    b = InputBox("b")
    c = InputBox("c")
    d = InputBox("d")
    e = InputBox("e")
    
    suma = a + b + c + d + e
    promedio = (a + b + c + d + e) / 5
    cuadradoSuma = suma * suma
    
    
    MsgBox ("a: ") & a
    MsgBox ("b:") & b
    MsgBox ("c: ") & c
    MsgBox ("d:") & d
    MsgBox ("e: ") & e
    MsgBox ("suma: ") & suma
    MsgBox ("promedio") & promedio
    MsgBox ("cuadradoSuma:") & cuadradoSuma
End Sub

Sub SumPromCua(a, b, c, d, e, suma, promedio, cuadradoSuma)
    suma = a + b + c + d + e
    promedio = (a + b + c + d + e) / 5
    cuadradoSuma = suma * suma
End Sub
