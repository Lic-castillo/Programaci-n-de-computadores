Option explicit 
Sub senofor()
    Dim x as double
    
    x = InputBox("x")
    
    MsgBox seno(x)
End Sub

Function seno(x)
    Dim n As Long
    Dim suma As Double
    Dim TermSig As Double

    suma = 0
    For n = 0 To 14
        TermSig = (-1) ^ (n) * (x) ^ (2 * n + 1) / factorial(2 * n + 1)
        suma = suma + TermSig
    Next n
    
    seno = suma
End Function
