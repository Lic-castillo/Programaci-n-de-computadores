Option Explicit
Sub num()
    Dim num As String
    
    num = InputBox("num")
    
    MsgBox invertir(num)
End Sub

Function invertir(num)
    Dim invertido As String
    Dim digsig As String
    Dim n As Long
    
    invertido = ""
    For n = Len(num) To 1 Step -1
        digsig = Mid(num, n, 1)
        invertido = invertido & digsig
    Next n
    
    invertir = invertido
End Function
