Option Explicit
Sub principal()
    Dim n As Long
    Dim m As Long
    
    n = InputBox("n")
    
    For m = 2 To n - 1
        If (m = 0) Then
            MsgBox (n & " no es divisible por " & m)
        ElseIf (Int(n / m) * m = n) And (n <> 0) Then
            MsgBox n & " es multiplo de " & m
        Else
            MsgBox n & " no es multiplo de " & m
        End If
    Next m
End Sub
