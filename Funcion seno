Option Explicit
Sub principal()

    Dim m As Long

    m = InputBox("m")
    
    MsgBox ("El resultado es ") & factorial(m)
    
End Sub
    
Function factorial(m)
    Dim n As Long
    Dim TermSig As Double
    Dim producto As Double
      
    producto = 1
    For n = 1 To m Step 1
        TermSig = n
        producto = producto * TermSig
    Next n
    
    factorial = producto
End Function

Sub secundario()

    Dim suma As Double
    Dim x As Long
    
    x = InputBox("defina x ")
    

    
    MsgBox ("seno es igual a ") & sumita(x)
End Sub
Function sumita(x)
    Dim n As Long
    Dim suma As Double
    Dim TermSig As Double
    

    
    suma = 0
    For n = 0 To 14 Step 1
        TermSig = ((-1) ^ (n)) * ((x) ^ (2 * n + 1)) / factorial(2 * n + 1)
        suma = suma + TermSig
    Next n
    
    sumita = suma
End Function
